---

<section id="latest-posts" class="py-16 px-4 bg-gray-900">
  <div class="max-w-7xl mx-auto">
    <h2 class="text-3xl font-bold text-center mb-12 text-white">Últimos Artículos</h2>
    <div class="grid md:grid-cols-3 gap-8">
      {posts.map(post => (
        <article class="bg-white/5 rounded-xl overflow-hidden hover:bg-white/10 transition-colors">
          <div class="h-48 bg-purple-900/10 overflow-hidden">
          <iframe
            id="miniatureweb"
            title="Preview of a webpage"
            width="100%"
            height="100%"
            src={post.link}
            allow="fullscreen"
          ></iframe>
          </div>
          <div class="p-6">
            <time class="text-sm text-gray-400">{post.date}</time>
            <h3 class="text-xl font-semibold mb-2 text-white">{post.title}</h3>
            <p class="text-gray-400 mb-4">{post.excerpt}</p>
            <a href={post.link} class="text-purple-400 hover:text-purple-300" target="_blank" rel="noopener noreferrer">Leer más →</a>
          </div>
        </article>
      ))}
    </div>
  </div>
</section>